<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="dawclipse">
         <property
               name="appName"
               value="dawclipse">
         </property>
            <property
               name="lifeCycleURI"
               value="bundleclass://io.dwerle.dawclipse.rcp/io.dwerle.dawclipse.rcp.E4LifeCycle">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="io.dwerle.dawclipse.rcp.splashHandlers.InteractiveSplashHandler"
            id="io.dwerle.dawclipse.rcp.splashHandlers.interactive">
      </splashHandler>
   </extension>
</plugin>
